package dev.abel.springbootredis.domain;

import java.io.Serializable;

public class Player implements Serializable {

	private String id;
	private String bet;
	private float betValue2;
	private float profit;
	
	public Player(String id, String bet, float betValue, float profit) {
		this.id = id;
		this.bet = bet;
		this.betValue2 = betValue;
		this.profit = profit;
	}
	
	
	public double getbetValue() {
		return profit;
	}
	public void setbetValue(float betValue) {
		this.profit = betValue;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getBet() {
		return bet;
	}
	public void setBet(String bet) {
		this.bet = bet;
	}

	public float getProfit() {
		return profit;
	}

	public void setProfit(float profit) {
		this.profit = profit;
	}


	public float getBetValue() {
		return betValue2;
	}


	public void setBetValue(float betValue) {
		this.betValue2 = betValue;
	}
	
	

}
