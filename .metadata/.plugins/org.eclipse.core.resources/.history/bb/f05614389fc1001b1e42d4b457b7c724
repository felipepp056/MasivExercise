package dev.abel.springbootredis.domain;

public class Player {
	private String id;
	private String bet;
	private double money;
	
	public Player(String id, String bet, double money) {
		this.id = id;
		this.bet = bet;
		this.money = money;
	}
	
	
	
	public double getMoney() {
		return money;
	}
	public void setMoney(double money) {
		this.money = money;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getBet() {
		return bet;
	}
	public void setBet(String bet) {
		this.bet = bet;
	}
	
	

}
